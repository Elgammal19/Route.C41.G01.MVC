@model Department
@{
    ViewData["Title"] = "Delete Department";
}

<h1>Are You Sure To Delete This Department</h1>

<form asp-action="Delete" method="post">

    <div class="text-danger" asp-validation-summary="All"></div>

    <partial name="PartialViews/DetailsDeleteDepartmentPartial" />

    <input type="submit" value="Delete" class="btn btn-primary" /> |
    <a class="btn btn-light" asp-controller="Department" asp-action="Index">Cancel</a>
</form>

